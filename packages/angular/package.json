{
  "name": "@gammabowl/example-lerna-monorepo-client-angular",
  "version": "1.0.0",
  "description": "Angular Frontend client for the service",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc && ng-packagr -p package.json -c tsconfig.json",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "author": "",
  "license": "UNLICENSED",
  "keywords": [
    "angular"
  ],
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "entryFile": "index.ts"
    },
    "dest": "./dist",
    "allowedNonPeerDependencies": ["."]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../../coverage",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist/"
    ]
  },
  "dependencies": {
    "@ngrx/effects": "^12.4.0",
    "ng-oidc-client": "^2.0.4",
    "@angular/cli": "~13.3.1",
    "@angular/compiler-cli": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/core": "~12.2.2",
    "ng-packagr": "~12.2.2",
    "oidc-client": "^1.11.5",
    "typescript": "^4.3.5",
    "esbuild": "^0.14.36"
  },
  "peerDependencies": {
    "@ngrx/store": ">= 12.0.0 < 13",
    "ng-oidc-client": ">= 2.0.0",
    "oidc-client": ">= 1.11.0"
  }
}
